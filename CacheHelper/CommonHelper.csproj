<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45;net46;</TargetFrameworks>
    <Company>Salomon Tech</Company>
    <Copyright>Copyright (c) 2021, NARDELE SALOMON DE OLIVEIRA</Copyright>
    <NeutralLanguage />
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <RootNamespace>Salomon.Common.Helper</RootNamespace>
    <Authors>Nardele Salomon</Authors>
    <PackageId>Salomon.CommonHelpers</PackageId>
    <Version>1.3.4</Version>
    <AssemblyVersion>1.3.4.0</AssemblyVersion>
    <PackageProjectUrl>https://github.com/delsoft/CommonHelper/blob/main/README.md</PackageProjectUrl>
    <FileVersion>1.3.4.0</FileVersion>
    <AssemblyName>Salomon.Common.Helper</AssemblyName>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Salomon.Common.Helper.pfx</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

